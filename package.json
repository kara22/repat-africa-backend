{
  "name": "repat-africa-backend",
  "description": "The backend of repat.africa",
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "graphql-cli": "^3.0.14",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "mjml": "^4.6.2",
    "nodemailer": "^6.4.16",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prisma-binding": "^2.3.16"
  },
  "scripts": {
    "deploy": "prisma deploy --env-file variables.env",
    "start": "nodemon -e js,graphql -x node src/index.js",
